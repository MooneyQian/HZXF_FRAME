<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Manage.Core.Entitys.SYS_AppRegisterEntity,Manage.Core" table="SYS_AppRegister" lazy="false">
    <!--ID必须要有，column可以不是ID，但是name一定要设置为ID-->
    <id name="ID" column="ID" type="string" length="36">
      <generator class="assigned"/>
    </id>
    <property type="string" name="AppRegisterID" column="AppRegisterID" />
    <property type="string" name="AppName" column="AppName" />
    <property type="string" name="LoginVerifiedUrl" column="LoginVerifiedUrl" />
    <property type="string" name="HomePageUrl" column="HomePageUrl" />
    <property type="int" name="OrderNum" column="OrderNum" />
    <property type="int" name="RecordStatus" column="RecordStatus" />
  </class>
</hibernate-mapping>
